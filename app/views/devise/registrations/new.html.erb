
<div class = "contenedor">
  <div class="container">
   <center><h2>Registro de usuario</h2></center>
    <div class="jumbotron4">
      <center>
        <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
        <%= devise_error_messages! %>

        <div><%= f.label :email %>
        <%= f.email_field :email %></div><br/>

        <div><%= f.label :password %>
        <%= f.password_field :password %></div><br/>

        <div><%= f.label :password_confirmation %>
        <%= f.password_field :password_confirmation %></div><br/>

        <div><%= f.submit t('.sign_up', :default => "Registrar") %></div>
        <% end %>

        <%= render "devise/shared/links" %>

      </center>
    </div>
  </div>
</div>